{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "import/no-extraneous-dependencies": "off",
        "no-confusing-arrow": "off"
    }
}
